{
    "name": "desolatormagno/js-utilities",
    "description": "Set of js utilities for tables and views in general.",
    "type": "library",
    "keywords": [
        "desolatormagno",
        "js-utilities",
        "CRUD",
        "js"
    ],
    "homepage": "https://github.com/DesolatorMagno/js-utilities",
    "license": "MIT",
    "authors": [
        {
            "name": "Reny Ramos",
            "email": "renyramosgarcia@gmail.com",
            "homepage": "https://github.com/DesolatorMagno",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "DesolatorMagno\\JsUtilities\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DesolatorMagno\\JsUtilities\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0.1-dev"
        },
        "laravel": {
            "providers": [
                "DesolatorMagno\\JsUtilities\\JsUtilitiesServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
