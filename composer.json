{
    "name": "desolatormagno/js-utilities",
    "type": "library",
    "description": "Set of js utilities for tables and views in general.",
    "keywords": [
        "desolatormagno",
        "js-utilities",
        "CRUD",
        "js"
    ],
    "homepage": "https://github.com/desolatormagno/js-utilities",
    "license": "MIT",
    "authors": [
        {
            "name": "Reny Ramos",
            "email": "renyramosgarcia@gmail.com",
            "homepage": "https://github.com/DesolatorMagno",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~7.2"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "desolatormagno\\js-utilities\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "desolatormagno\\js-utilities\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
